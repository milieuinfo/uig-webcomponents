{
  "name": "uig-webcomponents",
  "version": "3.0.8",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "npm run build && npm run test:unit && npm run test:e2e",
    "test:bamboo": "npm run build && npm run test:e2e:bamboo && npm run test:unit:bamboo",
    "test:server:start": "http-server ./ &",
    "test:unit": "wct --config-file ./config/wct.local.config.json",
    "test:unit:bamboo": "wct --config-file ./config/wct.docker.config.json",
    "test:e2e": "build-storybook && npm run test:server:start && mocha --chrome --config ./config/mocha.config.json --es-module-specifier-resolution=node",
    "test:e2e:short": "npm run test:server:start && mocha --chrome --local --config ./config/mocha.config.json --es-module-specifier-resolution=node",
    "test:e2e:woserver": "mocha --chrome --local --config ./config/mocha.config.json --es-module-specifier-resolution=node",
    "test:e2e:bamboo": "build-storybook && npm run test:server:start && npm run test:e2e:bamboo:chrome && npm run test:e2e:bamboo:firefox",
    "test:e2e:bamboo:chrome": "mocha --grid --chrome --config ./config/mocha.config.json --es-module-specifier-resolution=node",
    "test:e2e:bamboo:firefox": "mocha --grid --firefox --config ./config/mocha.config.json --es-module-specifier-resolution=node",
    "start": "npm run build && npm run start:storybook",
    "start:storybook": "start-storybook -s ./ -p 8080",
    "build-storybook": "npm run build && build-storybook",
    "build": "npm install && node ./scripts/build.js && webpack --config ./config/webpack.config.js",
    "release": "npm run build && npm version $npm_package_config_version && git push && npm publish"
  },
